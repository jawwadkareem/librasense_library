  <!--CONTACT US SECTION-->
  <section id="contact_us-hero-container">
    <div class="container-fluid">
      <div class="container contact_us-container-css reveal">
        <div class="category-header" style="padding-top: 40px;">
          <h5 id="section-main-heading">CONNECT WITH <span style="color: #9BA4B5;">US</span></h5>
          <hr
            style="width: 100px; margin: auto; margin-top: 10px; border: 2px solid #13005A; border-radius: 30px; opacity: 1;">
        </div>
        <div class="row contact_us-row-css">
          <div class="contact_us-content-left col-lg-7">
            <h3 id="contact_us_heading">Send Your Request</h3>
            <form action="#" style="padding: 10px 50px;">
              <div class="input-row">
                <div class="input-group">
                  <label>Name</label>
                  <input id="contact_us-input-field" type="text" placeholder="Muhib Ullah">
                </div>
                <div class="input-group">
                  <label>Email Address</label>
                  <input id="contact_us-input-field" type="email" placeholder="muhib123@gmail.com">
                </div>
              </div>
              <div class="input-row">
                <div class="input-group">
                  <label>Contact Number</label>
                  <input id="contact_us-input-field" type="text" placeholder="0315-aage nhi bataonga">
                </div>
                <div class="input-group">
                  <label>Subject</label>
                  <input id="contact_us-input-field" type="text" placeholder="jisme GPA nhi ayegi">
                </div>
              </div>
              <div class="contact_us-message-container">
                <label>Message</label>
                <textarea rows="3" placeholder="Your Message"></textarea>
              </div>
              <button type="submit" id="contact_us-submit_btn">SUBMIT</button>
            </form>
          </div>
          <div class="contact_us-content-right col-lg-5" style="background-color: #212A3E">
            <h1 id="contact_us-right-content-heading">LET'S CHAT!</h1>
            <p id="contact_us-right-content-text">Wheather you have a question, want to learn more about our services or
              simply want to connect</p>
            <p id="contact_us-right-content-text">Feel free to send us a message in the contact form!</p>
            <img id="contact_us-img" src="images/contact_us-image.png" alt="nhi hai">
          </div>
